[run]
source = .
omit = 
    */venv/*
    */env/*
    */site-packages/*
    */tests/*
    test_*.py
    *_test.py
    setup.py
    conftest.py
    */migrations/*
    htmlcov/*
    .pytest_cache/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml